    },
    "autoload-dev": {
        "psr-4": {
            "FireBender\\Laravel\\PictureWorks\\Tests\\": "tests/"
        }
    },    
    "extra": {
        "laravel": {
            "providers": [
                "FireBender\\Laravel\\PictureWorks\\Providers\\PackageServiceProvider"
            ]
        }
    }
}
