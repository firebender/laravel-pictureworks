{
    "name": "firebender/laravel-pictureworks",
    "description": "Pictureworks legacy test Laravel package",
    "license": "MIT",
    "authors": [
        {
            "name": "Jay Arabia",
            "email": "firebender.me@gmail.com"
        }
    ],
    "require-dev": {
        "firebender/laravel-dumper": "dev-master",
        "orchestra/testbench": "^6.7"
    },
    "autoload": {
        "psr-4": {
            "FireBender\\Laravel\\PictureWorks\\": "src/",
            "FireBender\\Laravel\\PictureWorks\\Database\\Factories\\": "database/factories/",
            "FireBender\\Laravel\\PictureWorks\\Database\\Seeders\\": "database/seeders/"
